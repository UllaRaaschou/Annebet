        <Button x:Name="Bt_SletBehandling" Content="Slet &#xA;Behandling" HorizontalAlignment="Left" Margin="477,307,0,0" VerticalAlignment="Top" Height="39" Width="154" Click="Bt_SletBehandling_Click"/>

